<div style="background: ">
	<h1>
	  <%= @note.title %>
	</h1>
	<p>
	  <%= @note.content %>
	</p>
	
</div>
<%= link_to "All Notes", notes_path, class: 'btn btn-default' %>
<%= link_to "Edit", edit_note_path(@note), class: 'btn btn-warning' %>
<%= link_to "Delete", note_path(@note), method: :delete, data: { confirm: "Are You Sure?" }, class: 'btn btn-danger' %>