 <% if user_signed_in? %>
     <%= link_to "New Note", new_note_path, class: 'btn btn-primary' %>
     <% @notes.each do |note| %>
		  <h2>
		    <%= link_to note.title, note %>
		  </h2>
		  <p>
		    <%= time_ago_in_words(note.created_at) %>
		  </p>
		<% end %>
    <% else %>
      <%= link_to "Log In", new_user_session_path, class: 'btn btn-success' %> or 
      <%= link_to "Register new accaunt", new_user_registration_path, class: 'btn btn-warning' %>

    <% end %>